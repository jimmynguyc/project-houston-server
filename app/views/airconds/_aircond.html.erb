<tr id = <%= "#{aircond.id}"%> class = aircond>
	<td><%=aircond.id%></td>
	<td class = "ac_status"><%=aircond.status%></td>
	<td><%=aircond.mode%></td>
	<td><%=aircond.fan_speed%></td>
	<td><%=aircond.temperature%></td>		
	<td style='width:100px' >
		<%= button_to 'Edit', edit_aircond_path(aircond), method:'get' ,style: "width:100%"%>
	</td>
	<td style='width:100px' >
		<%= button_to 'Set Timer', set_timer_path(aircond),method:'get',style: "width:100%"%>
	</td>
</tr>
